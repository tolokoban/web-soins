<?php
$ROLE = "USER";

/************************************************************
   { traceId: <number>, data: <string> }

   traceId: ID of the trace. Must belong to the current user.
   data: Base64 representation of a JPEG image.

   Errors:
   -1: Missing `traceId`.
   -2: Missing `data`.
************************************************************/
function execService( $arg ) {
    if (!array_key_exists("traceId", $arg)) return -1;
    if (!array_key_exists("data", $arg)) return -2;

}
?>
